<h4>Popular Queries</h4>
<div class="row">
  {{#if foods}}
    {{#each foods}}
      <div class="col s12 m4">
        <div class="card">
          <div class="card-content center-align">
            <p>{{name}}</p>
          </div>
          <div class="card-content center-align">
            <p>{{edible}}</p>
          </div>
        </div>
      </div>

    {{/each}}
  {{else}}
    <p>
      Food not available
    </p>
  {{/if}}
</div>